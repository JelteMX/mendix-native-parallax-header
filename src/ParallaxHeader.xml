<?xml version="1.0" encoding="utf-8"?>
<widget id="mendix.parallaxheader.ParallaxHeader" pluginWidget="true" needsEntityContext="true" offlineCapable="true"
        supportedPlatform="Native"
        xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd">
    <name>Parallax Header</name>
    <description>My widget description</description>
    <icon></icon>
    <properties>
        <propertyGroup caption="General">
            <property key="headerArea" type="widgets" required="true">
                <caption>Header area</caption>
                <description></description>
            </property>
            <property key="contentArea" type="widgets" required="true">
                <caption>Content area</caption>
                <description></description>
            </property>
        </propertyGroup>
        <propertyGroup caption="UI">
            <property key="uiStartCollapsed" type="boolean" defaultValue="true">
                <caption>Start header collapsed</caption>
                <description></description>
            </property>
            <property key="uiMinHeight" type="integer" required="true" defaultValue="0">
                <caption>Min height</caption>
                <description>Minimum height. This is default set to 50, but can be overridden by setting it here OR using the native-styles in your theme.</description>
            </property>
            <property key="uiMaxHeight" type="integer" required="true" defaultValue="0">
                <caption>Max height</caption>
                <description>Maximum height. This is default set to 200, but can be overridden by setting it here OR using the native-styles in your theme</description>
            </property>
        </propertyGroup>
    </properties>
</widget>
